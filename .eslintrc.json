{
  "root": true,
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [],
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "error"
  },
  "globals": {
    "$A": "readonly",
    "Lightning": "readonly",
    "Browser": "readonly",
    "ContentAsset": "readonly",
    "Label": "readonly",
    "Locale": "readonly",
    "Resource": "readonly"
  }
}
